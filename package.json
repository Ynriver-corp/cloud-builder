    "react-dom": "^17.0.2",
    "react-error-boundary": "^3.1.3",
    "react-facebook-pixel": "^1.0.4",
    "react-ga": "^3.3.0",
    "react-helmet": "^6.1.0",
    "react-hook-form": "^4.6.2",
    "react-joyride": "^2.3.0",
    "react-monaco-editor": "^0.45.0",
    "react-responsive": "^8.2.0",
    "react-router-dom": "^5.2.0",
    "react-virtualized": "^9.22.3",
    "react-window": "^1.8.6",
    "reactn": "^2.2.7",
    "resize-image": "^0.1.0",
    "styled-components": "^5.3.3",
    "url-assembler": "^2.1.1",
