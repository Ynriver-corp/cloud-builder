  },
  "devDependencies": {
    "react-email-editor": "^1.4.0",
    "copy-webpack-plugin": "^8.1.1",
    "@babel/core": "^7.13.10",
    "@babel/plugin-transform-runtime": "^7.13.10",
    "@babel/preset-env": "^7.13.12",
    "@babel/preset-react": "^7.12.13",
    "babel-loader": "^8.2.2",
    "clean-webpack-plugin": "^3.0.0",
    "compression-webpack-plugin": "^7.1.2",
    "css-loader": "^5.2.0",
    "css-minimizer-webpack-plugin": "^1.3.0",
    "html-webpack-plugin": "^5.3.1",
    "mini-css-extract-plugin": "^1.3.9",
    "react-pwa-install-prompt": "^1.0.5",
    "react-scripts": "^4.0.3",
    "source-map-explorer": "^2.5.2",
    "style-loader": "^2.0.0",
    "terser-webpack-plugin": "^5.1.1",
    "webpack": "^5.28.0",
    "webpack-bundle-analyzer": "^4.4.0",
    "webpack-cli": "^3.3.10",
    "webpack-dev-server": "^3.11.2"
  },
  "scripts": {
    "analyze": "source-map-explorer 'build/static/js/*.js'",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "start": "react-scripts start",
    "build": "GENERATE_SOURCEMAP=false react-scripts build",
    "startWebPack": "webpack-dev-server --config webpack.config.js",
    "buildWebPack": "webpack --config webpack.config.js",
    "buildWebPackToAnalize": "webpack --config webpack.config.js --profile --json > stats.json",
    "startBackEnd": "firebase emulators:start --only functions,auth,firestore --import=./dataEmulator --export-on-exit",
    "startFunctions": "firebase emulators:start --only functions",
    "deployDevIndexes": "firebase deploy --only firestore:indexes -P dev",
    "deployProdIndexes": "firebase deploy --only firestore:indexes -P default",
    "deployDev": "webpack --config webpack.config.js && firebase deploy -P dev",
    "deployProd": "webpack --config webpack.config.js && firebase deploy -P default",
    "deployDevFront": "webpack --config webpack.config.js && firebase deploy -P dev --except functions",
    "deployProdFront": "webpack --config webpack.config.js && firebase deploy -P default --except functions"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "sideEffects": [
    "*.scss",
    "*.css",
    "*polyfills.js",
    "*application.js",
    "*other-libs*"
  ]
}
